# IFBench Platform configuration
# Copy to .env and fill in your values.

# OpenRouter
API_BASE=https://openrouter.ai/api/v1
MODEL=arcee-ai/trinity-mini:free

# Prefer OPENROUTER_API_KEY; API_KEY is fallback for upstream compatibility.
OPENROUTER_API_KEY=
API_KEY=

# Optional attribution headers
OPENROUTER_HTTP_REFERER=
OPENROUTER_X_TITLE=IFBench Runner

# Generation params
TEMPERATURE=0
MAX_TOKENS=4096
WORKERS=4

# If your provider rejects seed, leave blank.
SEED=

# IFBench input
INPUT_FILE=vendor/IFBench/data/IFBench_test.jsonl
