//WindTerm WSL Outline
//Version: 1.0
{
	"type": "level",
	"patterns": [
		{
			"name": "prompt",
			"scope": "prompt.line.wsl",
			"match": "(*UTF)(?m)^([^\\n\\$#%\\x{e0b0}]*+[\\$#%]|[^\\n]*?\\x{e0b0}) [ \\t]*+(?<command>(\\S[^\\n\\x{e0b0}]*+|(?!\\n)))$"
		}
	],
	"regions": [
		{
			"name": "prompt",
			"title": "${command}",
			"icon": "king::Label",
			"sequence": ["prompt"]
		}
	]
}
