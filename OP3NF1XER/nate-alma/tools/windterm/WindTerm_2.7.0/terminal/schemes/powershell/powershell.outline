//WindTerm PowerShell Outline
//Version: 1.0
{
	"type": "level",
	"patterns": [
		{
			"name": "prompt",
			"scope": "prompt.line.powershell",
			"match": "(*UTF)(?m)^(PS [^\\n>]*+>|[^\\n❯]*+\\n❯) [ \\t]*+(?<command>(\\S[^\\n]*+|(?!\\n)))$"
		}
	],
	"regions": [
		{
			"name": "prompt",
			"title": "${command}",
			"icon": "king::Label",
			"sequence": ["prompt"]
		}
	]
}
