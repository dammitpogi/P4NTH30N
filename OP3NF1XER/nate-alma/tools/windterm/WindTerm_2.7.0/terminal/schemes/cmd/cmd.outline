//WindTerm Cmd Outline
//Version: 1.0
{
	"type": "level",
	"patterns": [
		{
			"name": "prompt",
			"scope": "prompt.line.cmd",
			"match": "(?m)^([^\\n\\\\>#λ]*+\\\\[^\\n>#λ]*+((?<!-)>|#)|λ)(?<command>[^\\n\\S]*+(\\S[^\\n]*+|(?!\\n)))$"
		}
	],
	"regions": [
		{
			"name": "prompt",
			"title": "${command}",
			"icon": "king::Label",
			"sequence": ["prompt"]
		}
	]
}
